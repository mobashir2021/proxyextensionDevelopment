{
    "manifest_version": 2,
    "name": "Proxy bw addons",
    "version": "1.0",
    "description": "A proxy extension",
    "background":{
        "scripts": ["background.js"]
    },
    "content_security_policy": "script-src 'self' 'sha256-B+Qe/KNUDtGDd/m1g5ycAq1DgpLs9ubKmYTlOHBogC8='; object-src 'self'",
    
   "web_accessible_resources": [ "inject.js", "injectsecond.js" ],
    "browser_action": {
        "default_icon": "images/icon16.png",
        "default_popup": "popup.html"
    },
    "incognito": "spanning",
    "content_scripts": [{
        "matches": ["<all_urls>"],
        "match_about_blank": true,
        "all_frames": true,
        "run_at": "document_start",
        "js": [ "content.js"]
}],
    "permissions": [
        "<all_urls>",
        "*://*/*",
        "https://digimarkon.com/",
        "tabs",
        "storage",
        "notifications", 
        "proxy",
        "webRequest",
        "webRequestBlocking",
        "alarms",
        "management",
        "browsingData"
        
      ]
}